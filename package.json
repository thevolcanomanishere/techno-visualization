	"dependencies": {
		"@remotion/cli": "^3.0.0",
		"@remotion/eslint-config": "^3.0.0",
		"@remotion/media-utils": "^3.0.0",
		"autoprefixer": "^10.4.8",
		"color-hash": "^2.0.1",
		"eslint": "^8.14.0",
		"parse-srt": "1.0.0-alpha",
		"postcss": "^8.4.16",
		"postcss-loader": "^7.0.1",
		"postcss-preset-env": "^7.7.2",
		"prettier": "^2.6.2",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"remotion": "^3.0.0",
		"tailwindcss": "^3.1.8",
